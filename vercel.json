{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "installCommand": "npm install --no-audit --no-fund --legacy-peer-deps",
  "buildCommand": "next build",
  "env": {
    "MONGODB_URI": "${MONGODB_URI}",
    "NODE_ENV": "production",
    "NEXT_PUBLIC_APP_NAME": "EcoClean Fidelidade"
  }
}
